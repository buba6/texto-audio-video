<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.arturo.texto_audio_video" >
    <uses-permission android:name="android.permission.CAMERA" />
    <uses-permission android:name="android.permission.RECORD_AUDIO" />
    <uses-permission android:name="android.permission.RECORD_VIDEO" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-feature android:name="android.hardware.camera" />
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".dos"
            android:label="@string/title_activity_dos" >
        </activity>
        <activity
            android:name=".tres"
            android:label="@string/title_activity_tres" >
        </activity>
        <activity
            android:name=".cuatro"
            android:label="@string/title_activity_cuatro" >
        </activity>
        <activity
            android:name=".cinco"
            android:label="@string/title_activity_cinco" >
        </activity>
        <activity
            android:name=".seis"
            android:label="@string/title_activity_seis" >
        </activity>
        <activity
            android:name=".v1"
            android:label="@string/title_activity_v1" >
        </activity>
    </application>

</manifest>
